type error = {
    code: int;
    message: string;
}

type failure = {
    id: int;
    error: error;
}

type response = {
    ~jsonrpc <python default="'2.0'">: string;
    id: int;
    result: abstract;
}

type request = {
    ~jsonrpc <python default="'2.0'">: string;
    id: int;
    method_ <json name="method">: string;
    params: abstract
}

type opts = {
    ~memo <python default="True">: bool;
    ~hash <python default="True">: bool;
}

type position = {
    line: int;
    character: int;
}

type list_notations_in_statement_params = {
    st: int;
    statement: string;
}

type ast_params = {
    st: int;
    text: string;
}

type ast_at_pos_params = {
    uri: string;
    position: position;
}

type get_state_at_pos_params = {
    ?opts: opts option;
    uri: string;
    position: position
}

type get_root_state_params = {
    ?opts: opts option;
    uri: string;
}

type start_params = {
    ?pre_commands: string option;
    ?opts: opts option;
    uri: string;
    thm: string;
}

type run_params = {
    ?opts: opts option;
    st: int;
    tac: string;
}

type state = {
    st: int;
    ?hash: int option;
    proof_finished: bool;
    feedback: (int * string) list
}

type goals_params = {
    st: int;
}

type goal_hyp = {
    names: string list;
    ?def_ <json name="def">: abstract option;
    ty: string;
}

type goal = {
    info: abstract;
    hyps: goal_hyp list;
    ty: string;
    ?pp: string option;
}

type goals_response = {
    goals : goal list;
    stack: (abstract list * abstract list) list;
    shelf: abstract list;
    given_up: abstract list
}

type premises_params = {
    st: int;
}

type premises_response = abstract

type inspect = [
    InspectPhysical <json name="Physical"> |
    InspectGoals <json name="Goals">
]

type state_equal_params = {
    kind: inspect list;
    st1: int;
    st2: int;
}

type state_equal_response = bool

type state_hash_params = {
    st: int;
}

type state_hash_response = int

type set_workspace_params = {
    debug: bool;
    root: string;
}

type toc_params = {
    uri: string;
}

type toc_response = (string * abstract) list
